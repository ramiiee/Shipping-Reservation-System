<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
<hibernate-mapping>
    <class name="com.wipro.srs.bean.ScheduleBean" table="SRS_TBL_SCHEDULE" >
        <id name="scheduleID" type="string">
            <column name="SCHEDULEID" not-null="true" />
        </id>
        <property name="routeID" type="string">
            <column name="ROUTEID" />
        </property>
        <property name="shipID" type="string">
            <column name="SHIPID" />
        </property>
        <property name="startDate" type="date">
            <column name="STARTDATE" />
        </property>
      </class>
</hibernate-mapping>